config_version=4

[application]

config/name="GdTweener Integration Tests"
run/main_scene="res://test_scene.tscn"
config/use_hidden_project_data_directory=false
run/flush_stdout_on_print=true
boot_splash/show_image=false

[audio]

default_bus_layout="res://audio_bus_layout.tres"

[autoload]

tweens_controller="*res://TweensController.gdns"

[debug]

